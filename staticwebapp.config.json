{
    "trailingSlash": "auto",
    "routes": [
        {
            "route": "/*",
            "rewrite": "/",
            "allowedRoles": [
                "authenticated",
                "anonymous"
            ]
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*"
        ]
    }
}